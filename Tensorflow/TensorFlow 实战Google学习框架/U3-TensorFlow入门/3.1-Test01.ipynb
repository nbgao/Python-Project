    "import tensorflow as tf\n",
    "\n",
    "a = tf.constant([1.0, 2.0], name = \"a\")\n",
    "b = tf.constant([2.0, 3.0], name = \"b\")\n",
    "result = a + b\n",
    "sess = tf.Session()\n",
    "sess.run(result)\n",
    "\n",
    "# 获取当前默认的计算图\n",
    "print(a.graph is tf.get_default_graph())"
   ]
